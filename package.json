{
  "name": "@inkline/cli",
  "version": "1.1.2",
  "description": "CLI for Inkline, the intuitive UI Components library that gives you a developer-friendly foundation for building high-quality, accessible, and customizable Vue.js 3 Design Systems.",
  "scripts": {
    "start": "npm run build && node --experimental-specifier-resolution=node lib/index.mjs",
    "dev": "vite",
    "build": "unbuild",
    "postbuild": "ts-node scripts/postbuild.ts",
    "lint": "eslint --ext .vue,.ts,.tsx,.js,.jsx src",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:ci": "vitest run --coverage",
    "test:dev": "vitest",
    "prerelease": "npm-run-all test build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "devDependencies": {
    "@grozav/eslint-config": "1.0.2",
    "@types/shelljs": "0.8.11",
    "eslint": "8.12.0",
    "np": "^7.6.2",
    "npm-run-all": "4.1.5",
    "pathe": "0.3.0",
    "shelljs": "0.8.5",
    "ts-node": "10.7.0",
    "typescript": "^4.5.4",
    "unbuild": "0.7.2",
    "vite": "^2.9.0",
    "vitest": "0.8.3"
  },
  "dependencies": {
    "@grozav/logger": "^1.0.0",
    "@grozav/utils": "^1.1.2",
    "@inkline/config": "^1.3.4",
    "chalk": "^4.1.2",
    "commander": "^9.3.0"
  },
  "main": "index.js",
  "module": "index.mjs",
  "jsdelivr": "index.js",
  "unpkg": "index.js",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "inkline": "./index.js"
  }
}
