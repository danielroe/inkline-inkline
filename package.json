{
  "name": "inkline",
  "version": "2.3.1",
  "description": "CLI for Inkline, the intuitive UI Components library that gives you a developer-friendly foundation for building high-quality, accessible, and customizable Vue.js 3 Design Systems.",
  "scripts": {
    "start": "npm run build && node --experimental-specifier-resolution=node lib/index.mjs",
    "dev": "vite",
    "build": "unbuild && cp package.json lib",
    "postbuild": "ts-node scripts/postbuild.ts",
    "lint": "eslint --ext .vue,.ts,.tsx,.js,.jsx src",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:ci": "vitest run --coverage",
    "test:dev": "vitest",
    "prerelease": "npm-run-all test build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "devDependencies": {
    "@grozav/eslint-config": "^1.1.4",
    "@inkline/config": "^1.0.0",
    "@schemastore/package": "^0.0.6",
    "@types/object-inspect": "1.8.1",
    "@types/prettier": "^2.7.2",
    "@types/shelljs": "0.8.11",
    "eslint": "8.22.0",
    "np": "^7.6.2",
    "npm-run-all": "4.1.5",
    "pathe": "0.3.5",
    "shelljs": "0.8.5",
    "ts-node": "10.9.1",
    "typescript": "^4.5.4",
    "unbuild": "0.7.6",
    "vite": "3.0.9",
    "vitest": "0.22.1"
  },
  "dependencies": {
    "@grozav/logger": ">=1",
    "@grozav/utils": ">=1",
    "@inkline/config": ">=1",
    "chalk": "^4.1.2",
    "commander": "^9.3.0",
    "prettier": "^2.8.2",
    "object-inspect": "1.12.2"
  },
  "main": "index.js",
  "module": "index.mjs",
  "jsdelivr": "index.js",
  "unpkg": "index.js",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "inkline": "./index.js"
  },
  "license": "MIT"
}
