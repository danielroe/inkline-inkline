{
  "name": "@inkline/config",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run build && node --experimental-specifier-resolution=node lib/index.mjs",
    "dev": "vite",
    "build": "unbuild",
    "postbuild": "ts-node scripts/postbuild.ts",
    "lint": "eslint --ext .vue,.ts,.tsx,.js,.jsx src",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:dev": "vitest",
    "prerelease": "npm-run-all test build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "devDependencies": {
    "@grozav/eslint-config": "1.0.2",
    "@types/color": "3.0.3",
    "@types/shelljs": "0.8.11",
    "csstype": "3.0.11",
    "eslint": "8.12.0",
    "jsdom": "19.0.0",
    "np": "^7.6.2",
    "npm-run-all": "4.1.5",
    "shelljs": "0.8.5",
    "ts-node": "10.7.0",
    "type-fest": "^2.13.0",
    "typescript": "^4.5.4",
    "unbuild": "0.7.2",
    "vite": "^2.9.0",
    "vitest": "0.8.3"
  },
  "dependencies": {
    "@grozav/utils": "1.1.1",
    "color": "4.2.3",
    "micromustache": "8.0.3"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "jsdelivr": "index.cjs",
  "unpkg": "index.cjs",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  }
}
