{
  "name": "@inkline/config",
  "version": "1.5.1",
  "scripts": {
    "start": "npm run build && node --experimental-specifier-resolution=node lib/playground.mjs",
    "dev": "vite",
    "build": "unbuild",
    "postbuild": "ts-node scripts/postbuild.ts",
    "lint": "eslint --ext .vue,.ts,.tsx,.js,.jsx src",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:dev": "vitest",
    "prerelease": "npm-run-all test build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts"
  },
  "devDependencies": {
    "@grozav/eslint-config": "1.0.2",
    "@types/shelljs": "0.8.11",
    "eslint": "8.22",
    "jsdom": "20.0.0",
    "np": "7.6.2",
    "npm-run-all": "4.1.5",
    "shelljs": "0.8.5",
    "ts-node": "10.9.1",
    "typescript": "4.5.4",
    "unbuild": "0.7.6",
    "vite": "3.0.9",
    "vitest": "0.22.1"
  },
  "dependencies": {
    "@grozav/utils": "latest",
    "@types/color": "3.0.3",
    "c12": "0.2.7",
    "color": "4.2.3",
    "csstype": "3.1",
    "micromustache": "8.0.3",
    "pathe": "0.3.5",
    "type-fest": "2.13.0"
  },
  "main": "index.js",
  "module": "index.mjs",
  "jsdelivr": "index.js",
  "unpkg": "index.js",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/alexgrozav"
  },
  "bugs": {
    "url": "https://github.com/inkline/inkline/issues"
  },
  "homepage": "https://inkline.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkline/config.git"
  },
  "license": "ISC"
}
