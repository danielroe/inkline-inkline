{
    "name": "@inkline/plugin",
    "version": "1.5.2",
    "scripts": {
        "start": "npm run build",
        "dev": "inkline-build watch",
        "dev:vite": "vite",
        "dev:vite:build": "vite build",
        "dev:webpack": "webpack serve",
        "dev:nuxt": "nuxt dev",
        "build": "unbuild",
        "postbuild": "tsx scripts/postbuild.ts",
        "lint": "eslint -c eslint.config.mjs src",
        "preview": "vite preview",
        "test": "vitest run --passWithNoTests",
        "test:ci": "vitest run --coverage",
        "test:dev": "vitest",
        "prerelease": "npm-run-all test build",
        "release": "np",
        "postrelease": "tsx scripts/postrelease.ts",
        "postversion": "tsx scripts/postversion.ts",
        "playground": "tsx scripts/playground.ts"
    },
    "devDependencies": {
        "@inkline/build": "workspace:^",
        "@inkline/config": "workspace:^",
        "@inkline/eslint-config": "workspace:^",
        "@inkline/inkline": "workspace:^",
        "@nuxt/module-builder": "^0.2.0",
        "@nuxt/schema": "^3.0.0",
        "@nuxtjs/eslint-config-typescript": "^12.0.0",
        "@types/node": "^20.8.10",
        "@types/shelljs": "0.8.11",
        "@vitejs/plugin-vue": "^5.0.4",
        "chokidar": "^3.5.3",
        "css-loader": "^6.7.1",
        "eslint": "^9.3.0",
        "html-webpack-plugin": "^5.5.0",
        "jsdom": "^21.0.0",
        "np": "^7.6.2",
        "npm-run-all": "4.1.5",
        "nuxt": "^3.3.3",
        "pathe": "^1.0.0",
        "sass": "^1.55.0",
        "sass-loader": "^13.1.0",
        "shelljs": "0.8.5",
        "style-loader": "^3.3.1",
        "tsx": "^4.10.5",
        "typescript": "^4.8.4",
        "unbuild": "^1.0.2",
        "unplugin": "^1.3.1",
        "vite": "^4.0.4",
        "vite-plugin-html": "^3.2.0",
        "vite-plugin-inspect": "^0.7.5",
        "vitest": "^0.27.1",
        "vue": "^3.3.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1"
    },
    "dependencies": {
        "@grozav/logger": "^1.0.1",
        "@grozav/utils": "^1.1.6",
        "@nuxt/kit": "^3.0.0"
    },
    "peerDependencies": {
        "@inkline/config": "workspace:^",
        "@inkline/inkline": "workspace:^"
    },
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "jsdelivr": "lib/index.js",
    "unpkg": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*",
        "LICENSE",
        "README.md"
    ],
    "exports": {
        ".": {
            "require": "./lib/index.js",
            "import": "./lib/index.mjs",
            "types": "./lib/index.d.ts"
        },
        "./esbuild": {
            "require": "./lib/esbuild.js",
            "import": "./lib/esbuild.mjs",
            "types": "./lib/esbuild.d.ts"
        },
        "./nuxt": {
            "require": "./lib/nuxt.js",
            "import": "./lib/nuxt.mjs",
            "types": "./lib/nuxt.d.ts"
        },
        "./rollup": {
            "require": "./lib/rollup.js",
            "import": "./lib/rollup.mjs",
            "types": "./lib/rollup.d.ts"
        },
        "./vite": {
            "require": "./lib/vite.js",
            "import": "./lib/vite.mjs",
            "types": "./lib/vite.d.ts"
        },
        "./webpack": {
            "require": "./lib/webpack.js",
            "import": "./lib/webpack.mjs",
            "types": "./lib/webpack.d.ts"
        },
        "./*": "./lib/*"
    },
    "publishConfig": {
        "access": "public"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/alexgrozav"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "bugs": {
        "url": "https://github.com/inkline/inkline/issues"
    },
    "homepage": "https://inkline.io",
    "keywords": [
        "inkline",
        "vue",
        "vuejs",
        "plugin",
        "nuxt",
        "nuxtjs",
        "webpack",
        "vite"
    ],
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/inkline/plugin.git"
    }
}
