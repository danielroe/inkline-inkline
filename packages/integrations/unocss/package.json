{
  "name": "@inkline/unocss",
  "type": "module",
  "version": "1.2.1",
  "scripts": {
    "prepare:dev": "cp src/playground/index.template.html src/playground/index.html",
    "prepare:build": "tsx src/playground/generate.ts",
    "start": "npm run build",
    "dev": "npm run prepare:dev && vite",
    "build": "unbuild",
    "build:local": "npm run build && cd lib && cp ../package.json . && npm pack",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "build:vite": "npm run prepare:build && vite build",
    "postbuild": "tsx scripts/postbuild.ts",
    "lint": "eslint --ext .vue,.ts,.tsx,.js,.jsx src",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:ci": "vitest run --coverage --passWithNoTests",
    "test:dev": "vitest",
    "prerelease": "npm-run-all test build",
    "release": "np",
    "postrelease": "tsx scripts/postrelease.ts",
    "postversion": "tsx scripts/postversion.ts"
  },
  "devDependencies": {
    "@grozav/eslint-config": "1.2.0",
    "@inkline/config": "^2.0.0-rc.1",
    "@inkline/plugin": "file:../plugin/lib/inkline-plugin-1.5.2.tgz",
    "@types/shelljs": "^0.8.12",
    "chokidar": "^3.5.3",
    "eslint": "^9.2.0",
    "jsdom": "^24.0.0",
    "np": "^10.0.5",
    "npm-run-all": "4.1.5",
    "pathe": "^1.1.1",
    "sass": "^1.55.0",
    "shelljs": "0.8.5",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "unocss": "^0.60.0",
    "vite": "^5.2.11",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.8.4",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@grozav/utils": "^1.3.1",
    "unplugin": "^1.3.1"
  },
  "peerDependencies": {
    "@inkline/config": "^2.0.0-rc.1",
    "unocss": ">=0"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "jsdelivr": "index.cjs",
  "unpkg": "index.cjs",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkline/unocss.git"
  }
}
